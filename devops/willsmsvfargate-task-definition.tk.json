{
    "family": "willsmsvfargate-task",
    "containerDefinitions": [
        {
            "name": "nestjs-container",
            "image": "#{ecrimageurlg}#",
            "cpu": 256,
            "memory": 512,
            "portMappings": [
                {
                    "containerPort": 3000,
                    "hostPort": 3000,
                    "protocol": "tcp"
                }
            ],
            "essential": true,
            "environment": [
                {
                    "name": "environment",
                    "value": "#{environment}#"
                },
                {
                    "name": "PORT",
                    "value": "3000"
                },
                {
                    "name": "BUCKET_WILL",
                    "value": "#{BUCKET_WILL}#"
                },
                {
                    "name": "AWS_SECRET_ID",
                    "value": "#{AWS_SECRET_ID}#" 
                },
                {
                    "name": "AWSREGION",
                    "value": "#{AWSREGION}#"
                },
                {
                    "name": "QUEUE_GENERATE_PDF",
                    "value": "#{QUEUE_GENERATE_PDF}#"
                },
                {
                    "name": "GETSNSTOPICARN",
                    "value": "#{GETSNSTOPICARN}#"
                },
                {
                    "name": "PPERRORMANAGEMENT",
                    "value": "#{PPERRORMANAGEMENT}#"
                },
                {
                    "name": "QUEUE_PROCESS_PDF",
                    "value": "#{QUEUE_PROCESS_PDF}#"
                },
                {
                    "name":"QUEUE_WILLS_COMMUNICATIONS",
                    "value":"#{QUEUE_WILLS_COMMUNICATIONS}#"
                }
            ],
            "mountPoints": [],
            "volumesFrom": [],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/nestjs",
                    "awslogs-region": "us-east-2",
                    "awslogs-stream-prefix": "nestjs"
                }
            },
            "systemControls": []
        }
    ],
    "tags": [
        {
            "key": "Project",
            "value": "willc"
        },
        {
            "key": "Environment",
            "value": "#{environment}#"
        }
    ],
    "taskRoleArn": "arn:aws:iam::440744252508:role/CarlosBorrarECSRole",
    "executionRoleArn": "arn:aws:iam::440744252508:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "volumes": [],
    "placementConstraints": [],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "256",
    "memory": "512"
}